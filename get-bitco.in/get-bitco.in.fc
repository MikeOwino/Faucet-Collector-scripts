    public override int DoSolveFaucet()
    {
	
      //=============//
      //FaucetClaim
      //=============//
      if (GetBoolSetting("FaucetClaim"))
      {

		GoToUrl("https://get-bitco.in/dashboard");
	    Wait();
	    Wait(3);
        Log ("FaucetClaim");

        var ButtonClaimCheck = ElementByXPath("//button[contains(@onclick,'startRand') and @disabled ]");
        if (IsVisible(ButtonClaimCheck))
        {
           Log ("you already claim");
        } // end ButtonClaimCheck

        if (!IsVisible(ButtonClaimCheck))
        {
			  
          var Button_Claim = ElementByXPath("//button[contains(@onclick,'startRand')]");  
          if (IsVisible(Button_Claim))
		  {
		  Log ("Button_Claim");
		  Click(Button_Claim);		
		  }
		  
	      Wait(2);
		  
          var NotAvailable = ElementByXPath("//a[contains(text(),'Back to dashboard') or contains(text(),'Back to')]");
		  if (IsVisible(NotAvailable))
		  {
			Click(NotAvailable);
		  }
		  
		  Wait(2);

          invisible_Recaptcha_Function(); //call 
	
	      Wait(6);
		  
          var Success_Claim = ElementByXPath("//button[contains(@onclick,'startRand') and @disabled ]"); //("//div[@class='alert alert-success']")
          if (IsVisible(Success_Claim))
		  {
		  Log("FaucetClaim - Success_Claim ");  
		  ClaimSuccess(); //Marks the claim as a success
		  }
		  
	      Wait(3);
	
	      //GO AGAIN
		  GoToUrl("https://get-bitco.in/dashboard");
	      Wait();
	      Wait(3);
	
		} //ButtonClaimCheck - not visible		  
		
      } //end FaucetClaim
      //=============//


      //=============//
      //ADSurf
      //=============//
      if (GetBoolSetting("ADSurf"))
      {

		GoToUrl("https://get-bitco.in/dashboard/surf");
	    Wait();
	    Wait(4);
        Log ("ADSurf");
		
         var TimeToWaitForBreak = DateTime.Now.AddSeconds(150);
         while (true)
         {
			 
     	   GoToUrl("https://get-bitco.in/dashboard/surf");
           Wait(2);

           var SuccessCheck = ElementByXPath("//div[contains(@id,'ms-insert')]//span[contains(text(),'ongratulation')]");
           var FaucetCheck = ElementByXPath("//div[@id='tempo' and contains(text(),'00')]");
           var NotAvailable = ElementByXPath("//a[contains(text(),'Back to dashboard') or contains(text(),'Back to')]");

		   if (IsVisible(NotAvailable))
		   {
			 Click(NotAvailable);
             Log("ADSurf - break - not available anymore");
             break; //break the wait loop
		   }
		   
		   if (IsVisible(FaucetCheck))
           { 
             Wait();
			 Log("ADSurf - wait");	 
		   } //end - FaucetCheck - visible  
		   
		   if (IsVisible(SuccessCheck))
           {
			 Log("ADSurf - end");
             Wait(2);
             var SuccessMessage = ElementByXPath("//div[contains(@id,'ms-insert')]//span");
             Log(SuccessMessage.Text);
             ClaimSuccess();
             Wait(2);			 
             Driver.Navigate().Refresh(); 
		   } //end - SuccessCheck - visible  
			        
           if (DateTime.Now > TimeToWaitForBreak) //if the time above 65 sec 
           {
           Log("ADSurf - break - TimeToWaitForBreak");
           break; //break the wait loop
           }
         
         } //end while true	
		  
      } //end ADSurf
      //=============//

		GoToUrl("https://get-bitco.in/dashboard");
	    Wait();
	    Wait(4);  
	  
		return Time_Function(); //call to function time
	
        //This faucet has nothing to do here. We will let Faucet Collector handle it.
        return base.DoSolveFaucet();
    }
	
	



//========//
//DoLogin
//========//
 public override int DoLogin()
 {
	
  var ExitButton = ElementByXPath ("//a[contains(@href,'logout')]"); 

  //if i NOT Login - go to Login Page
  if (!IsVisible(ExitButton))
  {

        GoToUrl("https://get-bitco.in/login");
        Wait();		
        Wait(2);

        var Email_VAR = ElementByXPath("//input[@id='email']");      
        if (IsVisible(Email_VAR))
        {
         SetText(Email_VAR, GetSetting("Email"));
        }
        
        Wait(2);

        var Password_VAR = ElementByXPath("//input[@type='password']");      
        if (IsVisible(Password_VAR))
        {
         SetText(Password_VAR, GetPassword("Password"));
        }
		
		Wait(3);
		
		Recaptcha_Function(); //call
		
		Wait(2);		
		
        var ButtonLog = ElementByXPath("//button[@type='submit']");  
        if (IsVisible(ButtonLog))
        {
         Click(ButtonLog);
        }

		Wait(4);

        var Owner_Bug_Password_Wrong = ElementByXPath("//div[@class='alert alert-danger' and contains(text(),'assword') ]");  
        if (IsVisible(Owner_Bug_Password_Wrong))
        {
          Log("Owner_Bug_Password_Wrong - we disable the script for protection ");
          Disable();
          return 1;
        }
		
		Wait();		
		
  } // end ExitButton

        
       return base.DoLogin();
    }
